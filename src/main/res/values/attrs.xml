<?xml version="1.0" encoding="utf-8"?>

<!-- This declares attributes for the FontAwesome TextView -->
<resources>
    <declare-styleable name="FontAwesome">
        <attr name="type" format="string" />
    </declare-styleable>

    <!-- TextView with Outline -->
    <declare-styleable name="OutlineTextView">
        <attr name="outlineColor" format="color" />
        <attr name="outlineWidth" format="dimension" />
    </declare-styleable>
</resources>


